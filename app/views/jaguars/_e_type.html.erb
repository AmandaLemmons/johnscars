          <h5><%= link_to "V12 E-Type to Chevy V8 Conversion", root_path%></h5>
        </div><!--services box-->
      </div> <!--col 6-->
      <div class="col-xs-6 col-sm-4">
        <div class="services-box-smaller">
          <h5><%= link_to "6-cylinder E-Type to Ford V8 Conversion", root_path%></h5>
        </div><!--services box-->
      </div><!--col 6-->
    </div><!--row-->
    <div class="row">
      <div class=" col-xs-6 col-sm-4 col-sm-offset-3">
        <div class="services-box-smaller" >
          <h5><%= link_to "E-Type Parts", root_path%></h5>
        </div><!--services box-->
      </div> <!--col 6-->
    </div><!--row-->
