  <h5 class="center"> MORE DETAILS</h5>
    <table class="table table-bordered">
      <tbody>
        <tr>
          <td><em>Tire Size/Radius: </em><b> <%= @etype_v12.tire_size%></b></td>

          <% if @etype_v12.rear_axl_ratio_other.present? || @etype_v12.rear_axl_ratio == "Other - fill out in next box"%>
            <td><em>Rear Axle Ratio: </em> <b><%=@etype_v12.rear_axl_ratio_other%></b></td>
          <% else %>
            <td><em>Rear Axle Ratio: </em> <b><%=@etype_v12.rear_axl_ratio%></b></td>
          <% end %>
        </tr>
        <tr>
          <% if @etype_v12.speedo_number_other.present? || @etype_v12.speedo_number == "Other - fill out in next box"%>
            <td><em>Speedo #: </em> <b><%=@etype_v12.speedo_number_other%></b></td>
          <% else %>
            <td><em>Speedo #: </em> <b><%=@etype_v12.speedo_number%></b></td>
          <% end %>
        </tr>
        <tr>
          <% if @etype_v12.motor_missing == true %>
          <td><em>Motor Missing:</em> <b>Yes</b>
            <% else %>
            <% end %>
          </td>
          <% if @etype_v12.motor_burned == true %>
          <td><em>Motor Burned:</em> <b>Yes</b>
            <% else %>
            <% end %>
          </td>
        </tr>
        <tr>
          <% if @etype_v12.right_hand_drive == true %>
          <td><em>Right-hand drive:</em> <b>Yes</b>
            <% else %>
            <% end %>
          </td>
        </tr>
      </tbody>
    </table>
  </div>

